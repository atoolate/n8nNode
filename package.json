{
  "name": "n8n-nodes-ittes-ai",
  "version": "0.1.3",
  "description": "n8n custom node for Ittes AI API",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Ittes",
    "email": "info@ittes.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atoolate/n8nNode.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/IttesAiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/IttesAi/IttesAi.node.js",
      "dist/nodes/IttesAi/IttesAiModel.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@langchain/core": "^0.3.76",
    "@langchain/openai": "^0.6.12"
  }
}
